    "typescript": "^4.7.4"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 2
  },
  "dependencies": {
    "@well-known-components/pg-component": "^0.2.2",
    "node-pg-migrate": "^6.2.2"
